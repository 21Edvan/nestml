package functionWithWrongReturns:

    component WrongReturns:
        function foo1():
            return 3 # 1
        end
        
        function foo2() real:
            return  # 2: missing return
        end
        
        function foo3() string:
            return 4 # 3 cannot convert to string
        end
        
        function foo4() boolean:
            return "hi" # 4 cannot convert to boolean
        end
        
        function foo5() integer:
            return 3.5 # 5 is not compatible
        end
    end